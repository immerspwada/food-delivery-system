  "keywords": [
    "food-delivery",
    "react",
    "node",
    "express",
    "socket.io"
  ],
  "author": "Food Delivery Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "axios": "^1.12.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
